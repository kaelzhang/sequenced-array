{
  "name": "sequenced-array",
  "version": "1.0.8",
  "description": "The sequenced array class which maintains sorted order with time complexity O(logN)",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc ava --timeout=10s",
    "test-no-report": "NODE_DEBUG=sequenced-array nyc ava --timeout=10s --verbose",
    "lint": "eslint .",
    "posttest": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "files": [
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/sequenced-array.git"
  },
  "keywords": [
    "sequenced-array",
    "array",
    "order",
    "sorted order",
    "desc",
    "asc"
  ],
  "engines": {
    "node": ">= 6"
  },
  "ava": {
    "babel": false
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/sequenced-array/issues"
  },
  "devDependencies": {
    "ava": "^1.3.1",
    "codecov": "^3.2.0",
    "nyc": "^13.3.0",
    "eslint-config-ostai": "^2.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint": "^5.15.1"
  }
}
